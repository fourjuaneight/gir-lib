{
  "name": "Big Pixel Codespace",
  "dockerFile": "Dockerfile",
  "containerUser": "node",
  "appPort": 8080,
  "extensions": [
    "denoland.vscode-deno",
    "laurencebahiirwa.deno-std-lib-snippets"
  ],
  "settings": {
    "deno.enable": true,
    "deno.lint": true,
    "deno.suggest.imports.hosts": {
      "https://cdn.skypack.dev": false
    },
    "deno.unstable": true,
    "terminal.integrated.automationShell.linux" : "/usr/bin/zsh",
    "workbench.colorCustomizations": {
      "statusBar.background": "#161e2e",
      "statusBar.foreground": "#ffffff"
    }
  }
}