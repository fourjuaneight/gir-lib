{
  "name": "Angular Codespace",
  "dockerFile": "Dockerfile",
  "containerUser": "node",
  "remoteUser": "node",
  "appPort": 3000,
  "extensions": [
    "angular.ng-template",
    "fivethree.vscode-ionic-snippets",
    "johnpapa.angular2"
  ],
  "postCreateCommand": "npm install",
  "settings": {
    "angular.experimental-ivy": true,
    "angular.trace.server": "messages",
    "terminal.integrated.automationShell.linux" : "/usr/bin/zsh",
    "terminal.integrated.defaultProfile.linux": "zsh",
    "workbench.colorCustomizations": {
      "statusBar.background": "#e23238",
      "statusBar.foreground": "#263340"
    }
  }
}